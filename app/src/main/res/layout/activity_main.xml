<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity"
    android:orientation="vertical"
    android:background="#dce5ff"
    android:gravity="center"
    android:id="@+id/fondo">

    <LinearLayout
        android:id="@+id/sup"
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="75dp"
        android:background="@android:color/background_light">

 <ImageView
     android:layout_width="wrap_content"
     android:layout_height="match_parent"
     android:src="@drawable/cal2"
     android:id="@+id/imagenView"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imagenView2"
            android:src="@drawable/protillo"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imagenView3"
            android:src="@drawable/portela"/>


<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/imageView5"
    android:src="@drawable/fran1"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imagenView6"
            android:src="@drawable/jose"/>

        </LinearLayout>

    <LinearLayout
        android:id="@+id/inf"
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="75dp"
        android:background="@android:color/background_light">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/imageView11"
            android:src="@drawable/alfonso"
            android:layout_gravity="center_horizontal|top"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView10"
            android:src="@drawable/sonia"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView9"
            />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView8"
           />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView0"
            />
    </LinearLayout>



    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="275dp"
        android:orientation="horizontal"
        android:gravity="center">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="75dp"
            android:layout_height="fill_parent"
            android:background="#269517"
            android:id="@+id/galicia">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Galicia"/>

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="78dp"
            android:layout_height="fill_parent"
            android:background="#fff81b"
            android:id="@+id/asturias">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Asturias"/>

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="75dp"
            android:layout_height="fill_parent"
            android:background="#5582ff"
            android:id="@+id/sevilla">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sevilla"/>

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="78dp"
            android:layout_height="fill_parent"
            android:background="#ff4f41"
            android:id="@+id/cataluña">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Cataluña"/>

        </LinearLayout>



    </LinearLayout>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Coloca al priaguista en su provincia"
        android:id="@+id/textView"
        android:textSize="20dp"
        android:paddingTop="20dp"/>


</LinearLayout>
